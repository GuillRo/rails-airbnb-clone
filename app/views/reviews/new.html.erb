<div class="container">
  <div style="margin:100px;">
    <%= form_for [@flat, @review] do |f| %>
      <div class="form-group">
        <%= f.label :score %>
        <%= f.text_field :score, class: 'form-control' %>
      </div>
      <div class="form-group">
        <%= f.label :content %>
        <%= f.text_field :content, class: 'form-control' %>
      </div>
      <%= f.submit class: 'btn btn-primary' %>
    <% end %>
  </div>
</div>
