<div class="container">
  <%= form_for(@booking) do |f| %>
    <div class="form-group">
      <%= f.label :date_begin %>
      <%= f.text_field :date_begin, class: 'form-control' %>
    </div>
    <div class="form-group">
      <%= f.label :date_end %>
      <%= f.text_field :date_end, class: 'form-control' %>
    </div>
    <%= f.submit class: 'btn btn-primary' %>
  <% end %>
</div>
